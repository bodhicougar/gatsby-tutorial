    "gatsby": "^2.11.6",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-playground": "^1.0.6",
    "gatsby-plugin-sass": "^2.1.0",
    "gatsby-plugin-sharp": "^2.2.7",
    "gatsby-plugin-styled-components": "^3.1.0",
    "gatsby-source-contentful": "^2.1.14",
    "gatsby-source-filesystem": "^2.1.4",
